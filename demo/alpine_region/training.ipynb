   },
   "outputs": [],
   "source": [
    "train_sampler_builder.initialize(\n",
    "    shape=SHAPE, mask_missing=masks.values, \n",
    ")\n",
    "test_sampler_builder.initialize(\n",
    "    shape=SHAPE, mask_missing=masks.values, \n",
    ")\n",
    "\n",
    "train_sampler = train_sampler_builder.get_sampler()\n",
    "test_sampler = test_sampler_builder.get_sampler()"
   ]
  },
  {
